      " * @copyright (c) $CURRENT_YEAR, ${6:AUTHOR or COMPANY}",      
      " * ",       
      " * @access private",           
      " * @version ${7:1.0.0}",
      " * @since ${8:ASCOOS 1.0.0}",
      " * @since ${9:PHP 8.0.0}",      
      " * ",
      " */",
      "private ${1:TYPE} ${2:\\$PROPERTY}${10:=${11:\\$VALUE}};",
      " ",
      "${0:}"
    ]
  },   
