  "a - Define the ASCOOS CMS execution variable.": {
    "prefix": "a-g-add-define-run-cms",
    "description": "Add Define the CMS execution variable.",
    "body": [
      "defined (\"ALEXSOFT_RUN_CMS\") or die(\"Prohibition of Access.\");",
      " ",
      "${0:}"      
    ]
  },
  "New Global Constant in ASCOOS CMS": {
    "prefix": "a-g-new-global-constant",
    "description": "Add global constant",
    "body": [
      "/**",
      " * @const ${1:CONSTANT} ${2:VALUE}",
      " * ",      
      "if (!defined (\"${1:CONSTANT}\")) define(\"${1:CONSTANT}\", \"${2:VALUE}\");",
      " ",
      "${0:}"      
    ]
  },  

  "New ASCOOS CMS Class": {
    "prefix": ["a-class", "a-g-new-class"],
    "description": "Add class.",
    "body": [
      "/**",
      " * @name ${1:Here write the name of the class.}",
      "${2: * @extends ${3:Here write the name of the class. Put extends class nameEXTENDS CLASS}}",
      " * ",
      " * ${4:Description}",
      " * ",
      "${5: * @author ${6:AUTHOR}}",
      "${7: * @link ${8:https://}",
      " * @created $CURRENT_YEAR-$CURRENT_MONTH-$CURRENT_DATE $CURRENT_HOUR:$CURRENT_MINUTE:$CURRENT_SECOND UTC+3",
      " * @updated ",
      " * @copyright (c) $CURRENT_YEAR, ${9:AUTHOR or COMPANY}",      
      " * ", 
      "${10: * ${11:@static${12: @readonly}}}",
      " * @version ${13:1.0.0}",
      " * @since ${14:ASCOOS 1.0.0}",
      " * @since ${15:PHP 8.0.0}",      
      " * ",
      " */",
      "${11:@static${12: @readonly }}class ${1:Here write the name of the class.}${3: extends ${3: EXTENDS CLASS}} {",
      " ",
      "   /** Properties */",
      " ",
      " ",
      "   /** Methods */",
      " ",
      " ",
      "   /**",
      "    * @method  __construct()",
      "    * ",
      "    * ",
      "    * ",
      "    * ",
      "    * ",
      "    */",
      "   public function __construct() {",
      "     /** CODE */",
      "   }",
      "} ",      
      "${0:}"
    ]
  },

