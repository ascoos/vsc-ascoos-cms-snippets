    "prefix": "a-g-new-property-private",
    "description": "Add private property in class.",
    "body": [
      "/**",
      " * @property ${1:TYPE} ${2:\\$PROPERTY}",
      " * ",
      " * ${3:Description}",
      " * ",
      " * @author ${4:AUTHOR}",
      " * @link ${5:AUTHOR LINK}",
      " * @created $CURRENT_YEAR-$CURRENT_MONTH-$CURRENT_DATE $CURRENT_HOUR:$CURRENT_MINUTE:$CURRENT_SECOND UTC+3",
